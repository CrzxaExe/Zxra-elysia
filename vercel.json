{
  "version": 2,
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/node",
      "config": {
        "runtime": "nodejs",
        "installCommand": "bun install",
        "buildCommand": "buqn run src/index.ts"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "./src/index.ts"
    }
  ]
}
