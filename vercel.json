{
  "version": 2,
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/node",
      "config": {
        "runtime": "nodejs",
        "installCommand": "bun install",
        "buildCommand": "bun build src/index.ts",
        "outputDirectory": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    }
  ]
}
