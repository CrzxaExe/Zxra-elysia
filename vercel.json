{
  "version": 2,
  "builds": [
    {
      "src": "index.ts", 
      "use": "@vercel/node",
      "config": {
        "zeroConfig": true,
        "functions": {
          "src/*.ts": {
            "runtime": "vercel-bun@1.1.31"
          }
        }
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/index.ts"
    }
  ]
}
